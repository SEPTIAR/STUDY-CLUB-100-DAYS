harga_mobil = 150_000_000  
penghasilan_bulanan = 6_000_000  
pengeluaran_bulanan = 3_000_000 + 1_000_000  

tabungan = 0 
tahun = 0  

# Menghitung waktu yang diperlukan Inas untuk membeli mobil
while tabungan < harga_mobil:
    tabungan += penghasilan_bulanan - pengeluaran_bulanan
    tahun += 1

print(f"Inas butuh {tahun} tahun untuk membeli mobil.")
