print("Operator Perbandingan")
angka1 = int(input("Masukkan angka1 = "))
angka2 = int(input("Masukkan angka2 = "))

print(angka1, ">", angka2, " = ", angka1 > angka2)
print(angka1, "<", angka2, " = ", angka1 < angka2)
print(angka1, ">=", angka2, " = ", angka1 >= angka2)
print(angka1, "<=", angka2, " = ", angka1 <= angka2)
print(angka1, "==", angka2, " = ", angka1 == angka2)
print(angka1, "!=", angka2, " = ", angka1 != angka2)
